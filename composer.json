{
    "name": "flatplane/flatplane",
    "type": "library",
    "description": "Flatplane is a frontend to TCPDF to create scientific reports with automatic formatting",
    "keywords": ["Flatplane","PDF","typesetting","typography","report","article","LaTeX","TeX"],
    "require": {
        "php": ">=5.5",
        "ext-gd": "*",
        "ext-SimpleXML": "*",
        "ext-curl": "*",
        "tecnick.com/tcpdf": ">=6.0.075",
        "symfony/process": "2.5.*@dev",
        "symfony/stopwatch": "dev-master"
    },
    "license": "LGPLv3",
    "authors": [
        {
            "name": "Nikolai Neff",
            "email": "admin@flatplane.de",
            "homepage": "https://flatplane.de"
        }
    ],
    "autoload": {
        "psr-4": {"de\\flatplane\\": "classes/"}
    },
    "suggest": {
        "ext-imagick": "*"
    }
}
